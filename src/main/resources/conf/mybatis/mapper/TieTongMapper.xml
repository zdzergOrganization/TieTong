<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.tietong.mapper.TieTongMapper"> 
	<select id="getUserByName" resultType="com.tietong.dao.User">
		SELECT
    		id,
    		user_name as username, 
    		password
    	FROM pot.account_info a
		WHERE
			a.user_name = #{username} 
	</select>
</mapper> 
